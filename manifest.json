{
	"manifest_version": 3,
	"name": "Twink Checker",
	"version": "3.0",
	"description": "Я ебу, как оно работает!",
	"permissions": ["storage", "activeTab", "tabs", "downloads", "scripting"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
		}
	},  
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}
